{
  "name": "projectlocker",
  "version": "1.0.0",
  "description": "Frontend for the projectlocker project",
  "main": "index.js",
  "scripts": {
    "test": "jest __tests__",
    "test:watch": "jest __tests__ --watch",
    "build": "webpack -d",
    "build:prod": "webpack -p",
    "start": "webpack -d --watch",
    "snyk-protect": "snyk protect",
    "lint": "prettier --check app __tests__",
    "lint:fix": "prettier --write app __tests__",
    "prepare": "yarn snyk-protect"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/react-treebeard/.+\\\\.js$",
      "node_modules/(?!panda-session).+\\\\.js$"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "./jestSetup.jsx"
    ]
  },
  "author": "Andy Gallagher <andy.gallagher@theguardian.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": ">=4",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-junit": "^4.0.0",
    "lint-staged": ">=10",
    "moxios": "^0.4.0",
    "object.entries": "^1.1.1",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0",
    "sinon": "^8.0.4",
    "source-map-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.3",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.19.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.15",
    "lodash.omit": "^4.5.0",
    "moment": "^2.20.1",
    "raven-js": "^3.24.1",
    "react": "^16.12.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.12.0",
    "react-multistep": "^3.6.16",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-sortable-table": "^1.3.0",
    "snyk": "^1.316.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "yarn": "^1.21.1"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
